<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/profile_list_relative_layout"
    android:orientation="vertical"
    android:layout_width="fill_parent" android:layout_height="60dp" android:padding="10dp" android:layout_gravity="center_vertical">
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/list_website"
        android:layout_alignParentTop="true"
        android:singleLine="true"
        android:text="@string/profile_list_website"
        android:textSize="18dp"
        android:textStyle="bold"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/list_username"
        android:singleLine="true"
        android:textSize="14dp"
        android:text="temp"
        />
</LinearLayout>